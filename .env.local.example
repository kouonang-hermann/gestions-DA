# Configuration Base de Données Supabase
DATABASE_URL="postgresql://postgres:[PASSWORD]@aws-1-eu-central-1.pooler.supabase.com:6543/postgres?pgbouncer=true"
DIRECT_URL="postgresql://postgres:[PASSWORD]@aws-1-eu-central-1.compute.amazonaws.com:5432/postgres"

# Configuration JWT
JWT_SECRET="your-super-secret-jwt-key-here"

